pragma solidity ^0.8.26;



contract calculator{
    uint256 public result;

    function add_one(uint256 number1, uint256 number2) public pure returns (uint256){
        return number1+number2;
    }
    function subtract(uint256 number1, uint256 number2) public pure returns (uint256){
        return number1-number2;
    }
    function multiply(uint256 number1, uint256 number2) public pure returns (uint256){
        return number1*number2;
    }
    function divide(uint256 number1, uint256 number2) public pure returns(uint256){
        return number1/number2;
    }

}
